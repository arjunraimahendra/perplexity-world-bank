    "import requests\n",
    "import json\n",
    "\n",
    "def stakeholder_request_function(stakeholder: str):\n",
    "\n",
    "    endpoint_url = \"https://api.perplexity.ai/chat/completions\"\n",
    "\n",
    "    payload = {\n",
    "        \"model\": MODEL,\n",
    "        \"messages\": [\n",
    "            {\n",
    "                \"role\": \"system\",\n",
    "                \"content\": system_prompt\n",
    "            },\n",
    "            {\n",
    "                \"role\": \"user\",\n",
    "                \"content\": stakeholder_prompt.format(stakeholder=stakeholder)\n",
    "            }\n",
    "        ],\n",
    "        # \"max_tokens\": \"Optional\",\n",
    "        \"temperature\": 0.2,\n",
    "        \"top_p\": 0.9,\n",
    "        \"return_citations\": True,\n",
    "        \"search_domain_filter\": [],\n",
    "        \"return_images\": False,\n",
    "        \"return_related_questions\": False,\n",
    "        # \"search_recency_filter\": \"month\",\n",
    "        \"top_k\": 0,\n",
    "        \"stream\": False,\n",
    "        \"presence_penalty\": 0,\n",
    "        \"frequency_penalty\": 1\n",
    "    }\n",
    "\n",
    "    headers = {\n",
    "        \"Authorization\": f\"Bearer {PERPLEXITY_API}\",\n",
    "        \"Content-Type\": \"application/json\"\n",
    "    }\n",
    "\n",
    "    response = requests.request(\"POST\", endpoint_url, json=payload, headers=headers)\n",
    "\n",
    "    return json.loads(response.text)"
   ]
