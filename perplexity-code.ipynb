    "import re\n",
    "from docx import Document\n",
    "\n",
    "def markdown_to_word(markdown_string, output_file):\n",
    "    # Create a new Word document\n",
    "    doc = Document()\n",
    "\n",
    "    # Process the markdown string line by line\n",
    "    lines = markdown_string.split('\\n')\n",
    "\n",
    "    for line in lines:\n",
    "        # Handle headings\n",
    "        if line.startswith('# '):\n",
    "            doc.add_heading(line[2:], level=1)\n",
    "        elif line.startswith('## '):\n",
    "            doc.add_heading(line[3:], level=2)\n",
    "        elif line.startswith('### '):\n",
    "            doc.add_heading(line[4:], level=3)\n",
    "        # Handle lists\n",
    "        elif line.startswith('- '):\n",
    "            doc.add_paragraph(line[2:], style='List Bullet')\n",
    "        # Add paragraphs with markdown text (handles bold and italic)\n",
    "        else:\n",
    "            paragraph = doc.add_paragraph()\n",
    "            add_markdown_text(paragraph, line)\n",
    "\n",
    "    # Save the Word document\n",
    "    doc.save(output_file)\n",
    "\n",
    "def add_markdown_text(paragraph, text):\n",
    "    \"\"\"\n",
    "    This function processes the markdown text and adds it to a Word document paragraph,\n",
    "    converting markdown bold (**text**) and italic (*text*) elements.\n",
    "    \"\"\"\n",
    "    # Regular expression for finding bold (**text**) and italic (*text*)\n",
    "    markdown_patterns = [\n",
    "        (r'\\*\\*(.*?)\\*\\*\\s*', 'bold'),  # Find bold text\n",
    "        (r'\\*(.*?)\\*\\s*', 'italic')     # Find italic text\n",
    "    ]\n",
    "    \n",
    "    current_pos = 0\n",
    "    for pattern, font_style in markdown_patterns:\n",
    "        matches = list(re.finditer(pattern, text))\n",
    "        \n",
    "        for match in matches:\n",
    "            start, end = match.span()\n",
    "\n",
    "            # Add the text before the markdown formatting\n",
    "            if current_pos < start:\n",
    "                paragraph.add_run(text[current_pos:start])\n",
    "\n",
    "            # Add the formatted text\n",
    "            run = paragraph.add_run(match.group(1) + \" \")\n",
    "            if font_style == 'bold':\n",
    "                run.bold = True\n",
    "            elif font_style == 'italic':\n",
    "                run.italic = True\n",
    "\n",
    "            current_pos = end\n",
    "\n",
    "    # Add any remaining text after the last match\n",
    "    if current_pos < len(text):\n",
    "        paragraph.add_run(text[current_pos:])\n",
    "\n",
    "# Example markdown string\n",
    "markdown_string = \"\"\"\n",
    "# Heading 1\n",
    "This is **bold text that includes multiple words** and this is *showing italic text*.\n",
    "\n",
    "## Heading 2\n",
    "- Item 1\n",
    "- Item 2\n",
    "\n",
    "This is a regular paragraph with **bold text** and *italic text*.\n",
    "\"\"\"\n",
    "\n",
    "# Convert to Word\n",
    "output_file = \"output.docx\"\n",
    "markdown_to_word(markdown_string, output_file)\n"
   ]
